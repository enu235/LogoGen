{
  "name": "logogen",
  "version": "1.0.0",
  "description": "Simple image generation website for logos and icons",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "health": "./health-check.sh",
    "setup": "./start.sh",
    "clean": "rm -rf public/generated/* temp/*",
    "docker:build": "docker build -t logogen .",
    "docker:run": "docker run -p 3000:3000 --env-file .env logogen",
    "docker:up": "docker-compose up --build",
    "docker:down": "docker-compose down",
    "test": "echo 'No tests specified'"
  },
  "dependencies": {
    "express": "^4.19.2",
    "dotenv": "^16.4.5",
    "axios": "^1.7.2",
    "sharp": "^0.33.4",
    "cors": "^2.8.5",
    "helmet": "^7.1.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.4"
  },
  "keywords": [
    "image-generation",
    "logo",
    "icon",
    "ai",
    "openai"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
